{
	"$schema": "https://developer.microsoft.com/json-schemas/tsdoc/v0/tsdoc.schema.json",
	"noStandardTags": false,
	"reportUnsupportedHtmlElements": true,
	"supportForTags": {
		"@deprecated": true,
		"@label": true,
		"@link": true,
		"@packageDocumentation": true,
		"@param": true,
		"@privateRemarks": true,
		"@remarks": true,
		"@returns": true,
		"@typeParam": true,
		"@defaultValue": true,
		"@decorator": true,
		"@eventProperty": true,
		"@example": true,
		"@inheritDoc": true,
		"@override": true,
		"@readonly": true,
		"@sealed": true,
		"@see": true,
		"@throws": true,
		"@virtual": true,
		"@jsx": true,
		"@jsxRuntime": true,
		"@jsxFrag": true,
		"@jsxImportSource": true,
		"@alpha": true,
		"@beta": true,
		"@experimental": true,
		"@internal": true,
		"@public": true
	}
}
